<?xml version="1.0" encoding="UTF-8"?>
<imp:If xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:imp="http://www.example.org/imp"
    xsi:schemaLocation="http://www.example.org/imp ../../imp.model/model/imp.ecore">
  <cond xsi:type="imp:Binary"
      op="LEQ">
    <lhs xsi:type="imp:IntConst"
        value="10"/>
    <rhs xsi:type="imp:IntConst"/>
  </cond>
  <lhs xsi:type="imp:Assign"
      name="a">
    <exp xsi:type="imp:IntConst"
        value="1"/>
  </lhs>
  <rhs xsi:type="imp:Assign"
      name="a">
    <exp xsi:type="imp:IntConst"
        value="2"/>
  </rhs>
</imp:If>
