open class boa.Let {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		boa::Expr lhs := self.lhs;
		boa_dynamic::EvalRes vlhs := lhs.eval(ctx);
		boa_dynamic::Ctx nctx := boa_dynamic::Ctx.create();
		nctx.env.putAll(ctx.env);
		nctx.env.put(self.name, vlhs);
		result := self.rhs.eval(nctx);
	}
}

