open class boa.With {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		boa_dynamic::EvalRes vlhs := self.lhs.eval(ctx);
		boa_dynamic::EvalRes vrhs := self.rhs.eval(ctx);
		
		if(vlhs.oclIsKindOf(boa_dynamic::EvalMapRes)) {
			boa_dynamic::EvalMapRes mvlhs := vlhs;
			if(vrhs.oclIsKindOf(boa_dynamic::EvalMapRes)) {
				boa_dynamic::EvalMapRes mvrhs := vrhs;
				boa_dynamic::EvalMapRes ret := boa_dynamic::EvalMapRes.create();
				ret.values.putAll(mvlhs.values);
				ret.values.putAll(mvrhs.values);
				result := ret;
				
			} else {
				result := null;
			}
		} else {
			result := null;
		}
		
		
	}
}
 