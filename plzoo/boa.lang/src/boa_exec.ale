open class boa.Assign {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		boa_dynamic::EvalRes vlhs := self.lhs.eval(ctx);
		boa_dynamic::EvalRes vrhs := self.rhs.eval(ctx);
		
		if(vlhs.oclIsKindOf(boa_dynamic::EvalMapRes)) {
			EvalMapRes mvlhs := vlhs;
			if(mvlhs.values.containsKey(self.name)) {
				mvlhs.values.put(self.name, vrhs);
				result := vrhs;
			} else {
				result := null;
			}
		} else {
			result := null;
		}
	}
}

