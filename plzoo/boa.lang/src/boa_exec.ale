behavior boa_exec;

use execboa.MapService;
use execboa.MathService;
use execboa.SerializeService;

open class boa.File {
	@main
	def void eval() {
		Ctx ctx := boa_dynamic::Ctx.create(); 
		for(it in self.commands) {
			it.nextLine(ctx);
		}
	}
}

open class boa.TopLevelCmd {
	def void nextLine(Ctx ctx) {}
}

open class boa.Def {
	def void nextLine(Ctx ctx) {
		boa_dynamic::EvalRes e := self.expr.eval(ctx);
		(self.name +  ' = ' + e.serialize()).log();
		ctx.env.put(self.name, e);
	}
}

open class boa.Expr {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		result := null;
	}
	
	def void nextLine(Ctx ctx) {
		self.eval(ctx).serialize().log();
	}
	
	// TODO: asInt / asBool
}

open class boa.Var {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		if(ctx.env.containsKey(self.name)) {
			result := ctx.env.getFromMap(self.name);
		} else {
			result := null;
		}
	}
}


open class boa.BObject {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		EvalMapRes ret := boa_dynamic::EvalMapRes.create();
		for(x in self.fields) {
			EvalRes v := x.value.eval(ctx);
			ret.values.put(x.name, v);
		}
		result := ret;
	}
}

open class boa.App {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		boa_dynamic::EvalRes vlhs := self.lhs.eval(ctx);
		boa_dynamic::EvalRes vrhs := self.rhs.eval(ctx);
		 
		if(vlhs.oclIsKindOf(boa_dynamic::EvalFunRes)) {
			if(vlhs.oclIsKindOf(boa_dynamic::EvalBoundFunRes)) {
				boa_dynamic::EvalBoundFunRes fct := vlhs;
				boa_dynamic::Ctx callCtx := boa_dynamic::Ctx.create();
				callCtx.env.putAll(fct.ctx.env);
				callCtx.env.put(fct.name, vrhs);
				callCtx.th.replaceWith(fct.th);
				EvalRes fe := fct.exp.eval(callCtx);
				if(fe.oclIsKindOf(boa_dynamic::EvalFunRes)) {
					boa_dynamic::EvalFunRes fun := fe;
					boa_dynamic::EvalBoundFunRes tmp := boa_dynamic::EvalBoundFunRes.create();
					tmp.exp := fun.exp;
					tmp.ctx := fun.ctx;
					tmp.name := fun.name;
					tmp.th.replaceWith(fct.th);
					result := tmp;
				} else {
					result := fe;
				}
			} else {
				boa_dynamic::EvalFunRes fct := vlhs;
				boa_dynamic::Ctx callCtx := boa_dynamic::Ctx.create();
				callCtx.env.putAll(fct.ctx.env);
				callCtx.env.put(fct.name, vrhs);
				callCtx.th.replaceWith(ctx.th);
				EvalRes fe := fct.exp.eval(callCtx);
				if(fe.oclIsKindOf(boa_dynamic::EvalFunRes)) {
					boa_dynamic::EvalFunRes fun := fe;
					boa_dynamic::EvalBoundFunRes tmp := boa_dynamic::EvalBoundFunRes.create();
					tmp.exp := fun.exp;
					tmp.ctx := fun.ctx;
					tmp.name := fun.name;
					tmp.th.replaceWith(ctx.th);
					result := tmp;
				} else {
					result := fe;
				}
			}
		} else {
			result := null;
		}
	}
}


