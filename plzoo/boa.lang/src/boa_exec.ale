open class boa.Fun {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		boa_dynamic::EvalFunRes ret := boa_dynamic::EvalFunRes.create();
		ret.exp := self.body;
		ret.ctx := ctx;
		ret.name := self.name;
		result := ret;
	} 
}

open class boa.If {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		EvalRes vcond := self.cond.eval(ctx);
		
		if(vcond.oclIsKindOf(boa_dynamic::EvalBoolRes)) {
			EvalBoolRes bvcond := vcond;
			if(bvcond.value) {
				result := self.thn.eval(ctx);
			} else {
				result := self.els.eval(ctx);
			}
		} else {
			result := null;
		}
	}
}


