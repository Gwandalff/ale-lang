		boa_dynamic::EvalRes vcopy := self.copy.eval(ctx);
		if(vcopy.oclIsKindOf(boa_dynamic::EvalMapRes)) {
			boa_dynamic::EvalMapRes mvcopy := vcopy;
			boa_dynamic::EvalMapRes ret := boa_dynamic::EvalMapRes.create();
			ret.values.putAll(mvcopy.values);	
			result := ret;
