open class boa.Project {
	def boa_dynamic::EvalRes eval(Ctx ctx) {
		boa_dynamic::EvalRes vexp := self.exp.eval(ctx);
		if(vexp.oclIsKindOf(boa_dynamic::EvalMapRes)) {
			boa_dynamic::EvalMapRes mvexp := vexp;
			if(mvexp.values.containsKey(self.name)) {
				boa_dynamic::EvalRes x := mvexp.values.get(self.name);
				
				if(x.oclIsKindOf(boa_dynamic::EvalFunRes)) {
					boa_dynamic::EvalFunRes func := x;			
					result := self.project(func, mvexp); 
				} else {
					result := x;
				}
			} else {
				result := null;
			}
		} else {
			result := null;
		}
	}
	
	def boa_dynamic::EvalRes project(boa_dynamic::EvalFunRes func, boa_dynamic::EvalMapRes mvexp) {			
		boa_dynamic::EvalBoundFunRes ret := boa_dynamic::EvalBoundFunRes.create();
		ret.exp := func.exp;
		ret.ctx := func.ctx;
		ret.name := func.name;
		ret.th.replaceWith(mvexp.values);
		result := ret;
	}
}

